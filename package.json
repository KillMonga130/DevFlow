{
  "name": "devflow",
  "version": "1.0.0",
  "description": "AI-powered code review training platform",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup": "node setup.js",
    "seed": "cd server && node seedData.js",
    "demo": "node demo-script.js"
  },
  "keywords": ["code-review", "education", "ai", "developer-training", "kiro"],
  "author": "Mueletshedzi Moses Mubvafhi",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}